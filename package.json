{
  "name": "uat-cms",
  "version": "3.0.0",
  "description": "CMS for Data-Driven Testing (DDT) - Capital One UAT - TanStack Edition",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon --ext js --watch server server/index.js",
    "client:dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/index.js",
    "start:static": "SERVE_STATIC=true node server/index.js",
    "start:prod": "NODE_ENV=production node server/index.js",
    "corporate": "npm run build && npm run start:static",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cms",
    "ddt",
    "testing",
    "playwright",
    "sqlite",
    "react",
    "tanstack",
    "tanstack-router",
    "tanstack-query"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.153.2",
    "@tanstack/react-store": "^0.8.0",
    "better-sqlite3": "^9.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.0"
  }
}
